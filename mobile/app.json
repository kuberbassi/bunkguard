{
  "expo": {
    "name": "AcadHub",
    "slug": "acad-hub-mobile-kuber",
    "description": "Your ultimate academic companion for tracking attendance, results, and skills.",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kuberbassi.acadhub",
      "infoPlist": {
        "CADisableMinimumFrameDurationOnPhone": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-foreground.png",
        "backgroundImage": "./assets/adaptive-icon-background.png",
        "backgroundColor": "#0A84FF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.kuberbassi.acadhubmobilekuber",
      "versionCode": 200,
      "usesCleartextTraffic": true,
      "softwareKeyboardLayoutMode": "pan",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "acadhub",
    "plugins": [
      "expo-secure-store",
      "expo-web-browser",
      "@react-native-google-signin/google-signin",
      "./plugins/withGradleWrapper",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#0A84FF"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.25"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ee6664bb-801c-4947-83e6-9cfb0a9d2eec"
      }
    },
    "jsEngine": "hermes",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "not.kuberb"
  }
}