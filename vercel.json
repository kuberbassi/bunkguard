{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/static/images/BunkGuard.png"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/static/sitemap.xml"
    },
    {
      "src": "/robots.txt",
      "dest": "/static/robots.txt"
    },
    {
      "src": "/google75750b6bbd7a51d3.html",
      "dest": "/static/google75750b6bbd7a51d3.html"
    },
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ]
}