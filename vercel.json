{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/public/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/public/static/images/BunkGuard.png"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/public/static/sitemap.xml"
    },
    {
      "src": "/robots.txt",
      "dest": "/public/static/robots.txt"
    },
    {
      "src": "/google75750b6bbd7a51d3.html",
      "dest": "/public/static/google75750b6bbd7a51d3.html"
    },
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
