{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "^/static/(.*)$",
      "dest": "/static/$1"
    },
    {
      "src": "^/favicon.ico$",
      "dest": "/static/images/BunkGuard.png"
    },
    {
      "src": "^/sitemap.xml$",
      "dest": "/static/sitemap.xml"
    },
    {
      "src": "^/robots.txt$",
      "dest": "/static/robots.txt"
    },
    {
      "src": "^/google75750b6bbd7a51d3.html$",
      "dest": "/static/google75750b6bbd7a51d3.html"
    },
    {
      "src": "^/(.*)$",
      "dest": "/run.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
