{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/favicon.ico",
      "dest": "/static/images/BunkGuard.png"
    },
    {
      "src": "/robots.txt",
      "dest": "/static/robots.txt"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/static/sitemap.xml"
    },
    {
      "src": "/google75750b6bbd7a51d3.html",
      "dest": "/static/google75750b6bbd7a51d3.html"
    },
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
